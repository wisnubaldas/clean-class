{
    "version": "1.0.0",
    "name": "wisnubaldas/clean-class",
    "description": "Bikin custom class di laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wisnubaldas\\CleanClass\\": "src/"
        }
    },
    "scripts": {
        "post-update-cmd": "Wisnubaldas\\CleanClass\\BootScript::get_routes",
        "post-package-install": [
            "Wisnubaldas\\CleanClass\\BootScript::get_routes"
        ],
        "post-install-cmd": [
            "Wisnubaldas\\CleanClass\\BootScript::get_routes",
            "php -v"
        ],
        "post-autoload-dump": [
            "Wisnubaldas\\CleanClass\\BootScript::get_routes"
        ],
        "post-create-project-cmd": [
            "php -v "
        ]
    },
    "authors": [
        {
            "name": "wisnubaldas",
            "email": "wisnubaldas@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.4 | ^8.0",
        "illuminate/console": "^6.20|^8.76|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wisnubaldas\\CleanClass\\CleanClassServiceProfider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
